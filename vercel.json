{
    "buildCommand": "npm run build",
    "framework": "vite",
    "cleanUrls": true,
    "trailingSlash": false,
    "outputDirectory": "dist",
    "buildOutput": {
        "cleanBuildCache": true
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}